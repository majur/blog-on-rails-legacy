<%= debug(params) if Rails.env.development? %>
<nav>
  <% if Setting.first.root_page_name.present? || Setting.first.root_page_menu? %>
    <% if Setting.first.root_page_menu? %>
      <%= link_to Setting.first.root_page_name, root_path %>
    <% else %>
    <% end %>
  <% else %>
  <% end %>
  <% Page.all.where('pages.active = ?', true).order(sort: :asc).each do |page| %>
    <%= link_to page.title, page %>
  <% end %>
</nav>
