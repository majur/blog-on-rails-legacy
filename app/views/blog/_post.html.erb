<% Post.all.each do |post| %>
<pre><p style="text-align: center"><%= post.created_at %></p><h1 style="line-height: 0.3;" link_to, post ><%= post.title %></h1>
<div style="text-align: justify"><%= post.sub_title %></div>
<div style="text-align: center"><%= button_to "Read more", post, :method => :get %></div></pre>
<% end %>
